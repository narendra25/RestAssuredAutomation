</File>

<Console name="STDOUT" target="SYSTEM_OUT">
<PatternLayout pattern="[%highlight{%d{ABSOLUTE} %p}{STYLE=Logback, bold}][%style{%C{1.}.%M}{cyan}] %m %red{%xEx{full}} %n"/>
</Console>

</Appenders>

<Loggers>
	
	<Logger name="com.Oneable.RestAssured.LoginStepDefintion.LoginUser" level="debug" additivity="false">
<AppenderRef ref="LoginUser" />
</Logger>

<Logger name="com.Oneable.RestAssured.LoginStepDefintion.RegisterStepDefinition" level="debug" additivity="false">
<AppenderRef ref="RegisterStepDefinition" />
</Logger>

<Logger name="com.Oneable.RestAssured.JBitbucketApi.StepDefintion.GetOrganzationRepoDeatails" level="debug" additivity="false">
<AppenderRef ref="GetOrganzationRepoDeatails" />
</Logger>

<Logger name="com.Oneable.RestAssured.JBitbucketApi.StepDefintion.GetOrganzationRepoDeatails" level="debug" additivity="false">
<AppenderRef ref="GetOrganzationRepoDeatails" />
</Logger>

<Logger name="com.Oneable.RestAssured.JBitbucketApi.StepDefintion.GetBrannchesDetailsStepDefinition" level="debug" additivity="false">
<AppenderRef ref="GetBrannchesDetailsStepDefinition" />
</Logger>

	<Logger name="com.Oneable.RestAssured.GiteaApi.StepDefintion.GetBranchDetail" level="debug" additivity="false">
<AppenderRef ref="GetBranchDetail" />
</Logger>

<Logger name="com.Oneable.RestAssured.GiteaApi.StepDefintion.GetBranchesDetails" level="debug" additivity="false">
