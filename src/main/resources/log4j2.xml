<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
<Appenders>
	<File name="LoginUser" fileName="LogReports/ApplicationLogs__${date:yyyy-MM-dd_HH-mm-ss}/LoginUser.log" append="true">
<PatternLayout pattern="[%highlight{%d{ABSOLUTE} %p}{STYLE=Logback, bold}][%style{%C{1.}.%M}{cyan}] %m %red{%xEx{full}} %n"/>
</File>

<File name="RegisterStepDefinition" fileName="LogReports/ApplicationLogs__${date:yyyy-MM-dd_HH-mm-ss}/RegisterStepDefinition.log" append="true">
