<PatternLayout pattern="[%highlight{%d{ABSOLUTE} %p}{STYLE=Logback, bold}][%style{%C{1.}.%M}{cyan}] %m %red{%xEx{full}} %n"/>
</File>

<File name="GetBranchDetail" fileName="LogReports/ApplicationLogs__${date:yyyy-MM-dd_HH-mm-ss}/GetBranchDetail.log" append="true">
<PatternLayout pattern="[%highlight{%d{ABSOLUTE} %p}{STYLE=Logback, bold}][%style{%C{1.}.%M}{cyan}] %m %red{%xEx{full}} %n"/>
</File>

<File name="GetBranchesDetails" fileName="LogReports/ApplicationLogs__${date:yyyy-MM-dd_HH-mm-ss}/GetBranchesDetails.log" append="true">
<PatternLayout pattern="[%highlight{%d{ABSOLUTE} %p}{STYLE=Logback, bold}][%style{%C{1.}.%M}{cyan}] %m %red{%xEx{full}} %n"/>
</File>

<File name="Getuserdetails" fileName="LogReports/ApplicationLogs__${date:yyyy-MM-dd_HH-mm-ss}/Getuserdetails.log" append="true">
