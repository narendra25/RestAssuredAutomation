<Logger name="com.Oneable.RestAssured.GiteaApi.StepDefintion.GetRepoDetails" level="debug" additivity="false">
<AppenderRef ref="GetRepoDetails" />
</Logger>

<Root level="info">
<AppenderRef ref="FILEOUT" level="Trace"/>
<AppenderRef ref="STDOUT" level="Trace"/>
</Root>
</Loggers>
</Configuration>
